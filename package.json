{
  "name": "divs",
  "version": "1.0.0",
  "description": "A collection of platform-agnostic component libraries for modern web development",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "build:marquee": "pnpm --filter marquee build",
    "build:wf-swiper": "pnpm --filter wf-swiper build",
    "dev:marquee": "pnpm --filter marquee build:watch",
    "dev:wf-swiper": "pnpm --filter wf-swiper build:watch",
    "release": "pnpm -r release",
    "release:marquee": "pnpm --filter marquee release",
    "release:wf-swiper": "pnpm --filter wf-swiper release"
  },
  "keywords": [
    "components",
    "library",
    "cdn",
    "webflow"
  ],
  "author": "Idrees Isse (https://github.com/idreezus)",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@rollup/plugin-terser": "^0.4.4",
    "eslint": "^9.38.0",
    "rollup": "^4.9.6"
  }
}
